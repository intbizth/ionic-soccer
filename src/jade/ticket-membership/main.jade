ion-view.ticket-membership
    ion-nav-title Ticket & Membership
    ion-nav-buttons(side="left")
        a.button.button-nav.button-icon.ion-ios-arrow-left(ng-if="isIOS", ng-click="back()")
            img.logo(
            src="./img/logo/navbar.png"
            srcset="./img/logo/navbar@2x.png 2x"
            )
    //ion-nav-buttons(side="right")
    //    a.button.button-clear(ng-click="")
    //        i.icon.ion-android-more-vertical

    ion-content.content(scroll="false")
        ion-tabs.tabs-striped.tabs-top.tabs-icon-left.tabs-background-positive.tabs-color-light(ng-show="permission.login")
            ion-tab(title="Ticket")
                ion-content.ticket.fix-tab
                    div(ui-view="ticket")
            ion-tab(title="Membership")
                ion-content.membership.fix-tab
                    div(ui-view="membership")

        ion-tabs.tabs-striped.tabs-top.tabs-icon-left.tabs-background-positive.tabs-color-light(ng-hide="permission.login")
            ion-tab(title="Ticket")
                ion-content.ticket.fix-tab
                    div(ui-view="ticket")
